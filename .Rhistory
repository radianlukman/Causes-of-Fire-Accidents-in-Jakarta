binormal=function(n,mu1,mu2,sd1,sd2,r)
{
x=matrix(0,n,2)
for(i in 1:n){
x[i,1]=mu1+sd1*z1[i]
x[i,2]=mu2+r*sd2*z1[i]+sd2*sqrt(1-r^2)*z2 [i]
}
return(x)
}
